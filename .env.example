# Security (Generate strong random strings)
SESSION_SECRET=your_strong_session_secret
IP_HASH_SALT=your_strong_ip_hash_salt

# PocketBase Connection
POCKETBASE_URL=http://127.0.0.1:8090 # Or your PocketBase instance URL
POCKETBASE_ADMIN_EMAIL=your_pocketbase_admin_email
POCKETBASE_ADMIN_PASSWORD=your_pocketbase_admin_password

# Running Environment
NODE_ENV=development # Or 'production'

# Optional: Logging Level (TRACE, DEBUG, INFO, WARN, ERROR, NONE - Default: INFO)
LOG_LEVEL=DEBUG

# Optional: Server Port (Default: 3000)
PORT=3000